    "import numpy as np\n",
    "\n",
    "def selu(z):\n",
    "    alpha = 1.6732632423543772\n",
    "    scale = 1.0507009873554805  \n",
    "    return scale * np.where(z >= 0, z, alpha * (np.exp(z) - 1))"
