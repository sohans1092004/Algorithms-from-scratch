   "source": [
    "import numpy as np\n",
    "\n",
    "def he_normal(fan_in, shape):\n",
    "    std = np.sqrt(2.0 / fan_in)\n",
    "    return np.random.normal(loc=0.0, scale=std, size=shape)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "4b984003-8e78-4220-8502-3565bf743b27",
